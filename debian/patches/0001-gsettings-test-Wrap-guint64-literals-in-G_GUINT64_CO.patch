From 536a1fc6cf108d4fff1315ac097dac344b6c3739 Mon Sep 17 00:00:00 2001
From: Iain Lane <iain@orangesquash.org.uk>
Date: Thu, 23 Jun 2016 15:56:38 +0100
Subject: [PATCH] gsettings test: Wrap guint64 literals in G_GUINT64_CONSTANT

This fixes test failures on some architectures.
---
 gio/tests/gsettings.c | 12 ++++++------
 1 file changed, 6 insertions(+), 6 deletions(-)

diff --git a/gio/tests/gsettings.c b/gio/tests/gsettings.c
index 86cc85c..7cbf775 100644
--- a/gio/tests/gsettings.c
+++ b/gio/tests/gsettings.c
@@ -1182,13 +1182,13 @@ test_simple_binding (void)
 
   g_settings_bind (settings, "uint64", obj, "uint64", G_SETTINGS_BIND_DEFAULT);
 
-  g_object_set (obj, "uint64", 12345, NULL);
-  g_assert_cmpuint (g_settings_get_uint64 (settings, "uint64"), ==, 12345);
+  g_object_set (obj, "uint64", G_GUINT64_CONSTANT (12345), NULL);
+  g_assert_cmpuint (g_settings_get_uint64 (settings, "uint64"), ==, G_GUINT64_CONSTANT (12345));
 
-  g_settings_set_uint64 (settings, "uint64", 54321);
-  u = 1111;
-  g_object_get (obj, "uint64", &u, NULL);
-  g_assert_cmpuint (u, ==, 54321);
+  g_settings_set_uint64 (settings, "uint64", G_GUINT64_CONSTANT (54321));
+  u64 = G_GUINT64_CONSTANT (1111);
+  g_object_get (obj, "uint64", &u64, NULL);
+  g_assert_cmpuint (u64, ==, G_GUINT64_CONSTANT (54321));
 
   g_settings_bind (settings, "int64", obj, "int64", G_SETTINGS_BIND_DEFAULT);
 
-- 
